{% endblock subtitle %}
{% block titlePanel %}
Promocion
{% endblock titlePanel %}
{% block action %}
        <div class="form-group form-actions">
    <div class="col-md-12 col-md-offset-6">
        <a href="{{ path('admin_promocion_index') }}" class="btn btn-sm btn-primary">
            <i class="hi hi-chevron-left"></i>
            Regresar
        </a>            <a href="{{ path('admin_promocion_edit', { 'id': promocion.id }) }}" class="btn btn-sm btn-warning">
