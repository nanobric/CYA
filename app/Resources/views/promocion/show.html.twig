            </a>    </div>
</div>{% endblock action %}
{% block bodyPanel %}
    <div class="table-responsive">
        <table class="table table-vcenter table-striped">
            <tbody>
                <tr>
                    <th>Id</th>
                        <td>{{ promocion.id }}</td>
                </tr>
                <tr>
                    <th>Name</th>
                        <td>{{ promocion.name }}</td>
                </tr>
                <tr>
                    <th>Code</th>
                        <td>{{ promocion.code }}</td>
                </tr>
                <tr>
                    <th>Percentage</th>
                        <td>{{ promocion.percentage }}</td>
                </tr>
            </tbody>
        </table>
    </div>
{% endblock bodyPanel %}
