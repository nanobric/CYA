{% extends 'base.html.twig' %}
{% block iconHead %} <i class="gi gi-list"></i>{% endblock iconHead %}
{% block pageTitle %}
    exchangeRateUSD
{% endblock pageTitle %}
{% block subtitle %} Lista {% endblock subtitle %}
    {% block action %}
        <div class="block-options pull-right">
            <a href="{{ path('admin_exchangeRateUSD_new') }}" data-original-title="Create a new entry"
               class="btn btn-alt btn-sm btn-success" data-toggle="tooltip" title=""><i class="fa fa-asterisk"></i></a>
        </div>
        {% endblock action %}
        {% block bodyPanel %}    <table id="example-datatable" class="table table-vcenter table-condensed table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Local</th>
                <th>Date</th>
                <th>Coin_id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for exchangeRateUSD in exchangeRateUSDs %}
            <tr>
                <td><a href="{{ path('admin_exchangeRateUSD_show', { 'id': exchangeRateUSD.id }) }}">{{ exchangeRateUSD.id }}</a></td>
                <td>{{ exchangeRateUSD.local }}</td>
                <td>{% if exchangeRateUSD.date %}{{ exchangeRateUSD.date|date('Y-m-d') }}{% endif %}</td>
                <td>{{ exchangeRateUSD.coinid }}</td>
                <td>                        <div class="btn-group">
                            <a href="{{ path('admin_exchangeRateUSD_show', { 'id': exchangeRateUSD.id }) }}" class="btn btn-xs btn-primary">show</a>
                        </div>                        <div class="btn-group">
                            <a href="{{ path('admin_exchangeRateUSD_edit', { 'id': exchangeRateUSD.id }) }}" class="btn btn-xs btn-primary">edit</a>
                        </div>                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock bodyPanel %}
{% block extraJS %} <script src="{{ asset('bundles/tscya/js/pages/tablesDatatables.js') }}"></script> <script>$(function(){ TablesDatatables.init(); });</script {% endblock extraJS %}
